<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.persist.mapper.user.EmployeeMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.example.persist.entity.user.Employee">
        <id column="id" property="id" />
        <result column="username" property="username" />
        <result column="realname" property="realname" />
        <result column="password" property="password" />
        <result column="tel" property="tel" />
        <result column="email" property="email" />
        <result column="dept_id" property="deptId" />
        <result column="inputtime" property="inputtime" />
        <result column="state" property="state" />
        <result column="admin" property="admin" />
        <result column="gender" property="gender" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, username, realname, password, tel, email, dept_id, inputtime, state, admin, gender
    </sql>

</mapper>
